{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id", "_", "_doc", "\/"] }],
    "react/prop-types": 0
    // "no-console": "error"
  }
}